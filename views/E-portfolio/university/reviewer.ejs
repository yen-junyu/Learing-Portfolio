<%- include('header') -%>
    <div class="container-fluid">
        <div class="row">
            <nav id="sidebarMenu" class="col-md-3 col-lg-2 d-md-block bg-light sidebar collapse" style="font-size:.875rem">
                <div class="sidebar-sticky pt-3">
                    <ul class="nav flex-column">
                        <li class="nav-item">
                            <a class="nav-link" href="#">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-users">
                                    <path d="M17 21v-2a4 4 0 0 0-4-4H5a4 4 0 0 0-4 4v2"></path>
                                    <circle cx="9" cy="7" r="4"></circle>
                                    <path d="M23 21v-2a4 4 0 0 0-3-3.87"></path>
                                    <path d="M16 3.13a4 4 0 0 1 0 7.75"></path>
                                </svg>
                                Applicant
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
                                    fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round"
                                    stroke-linejoin="round" class="feather feather-file">
                                    <path d="M13 2H6a2 2 0 0 0-2 2v16a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2V9z"></path>
                                    <polyline points="13 2 13 9 20 9"></polyline>
                                </svg>
                                Profile
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
            <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
                <% if(typeof schoolData != "undefined" && schoolData){ %>
                    <div class="d-flex align-items-center p-3 my-1 text-white-50 bg-primary rounded shadow-sm">
                        <div class="lh-100">
                            <h6 class="mb-0 text-white lh-100">Personal Information</h6>
                            <small>from <%= schoolData['studentInfo'].highSchool %></small>
                        </div>
                    </div>
                    <div class="p-1 bg-white rounded shadow-sm">
                        <div class="pt-3 ">
                            <p class="media-body pb-3 mb-0 lh-125 border-bottom border-gray">
                                <strong class="d-block text-gray-dark text-muted">Student Name: <span style="color: black;">
                                    <%= schoolData['studentInfo'].Name %></span></strong>
                            </p>
                        </div>
                        <div class="pt-3 ">
                            <p class="media-body pb-3 mb-0 lh-125 border-bottom border-gray">
                                <strong class="d-block text-gray-dark text-muted">DID address: <span style="color: black;">
                                    <%= schoolData['studentInfo'].account %></span></strong>
                            </p>
                        </div>
                        <div class="pt-3 ">
                            <p class="media-body pb-3 mb-0 lh-125 border-bottom border-gray">
                                <strong class="d-block text-gray-dark text-muted">High School: <span style="color: black;">
                                    <%= schoolData['studentInfo'].highSchool %></span></strong>
                            </p>
                        </div>
                        <div class="pt-3 ">
                            <p class="media-body pb-3 mb-0 lh-125 text-muted">
                                <strong class="d-block text-gray-dark ">E-portfolio public key : </strong>
                            </p>
                            <p class="small border-bottom border-gray">
                                <%= schoolData['studentInfo'].publicKey %>
                            </p>
                        </div>
                    </div>
                    <div class="d-flex align-items-center p-3 my-1 text-white-50 bg-primary rounded shadow-sm">
                        <div class="lh-100">
                            <h6 class="mb-0 text-white lh-100">School grade</h6>
                            <small>from <%= schoolData['studentInfo'].highSchool %></small>
                        </div>
                    </div>               
                    <div class="table-responsive">
                        <div class="pt-3 ">
                            <p class="media-body pb-3 mb-0 lh-125 border-bottom border-gray">
                                <strong class="d-block text-gray-dark text-muted">Subject grades:</strong>
                            </p>
                        </div>       
                        <table class="table table-striped table-sm small">
                                <thead>
                                    <tr>
                                        <th>NO</th>
                                        <th>學年度學期</th>
                                        <th>課名</th>
                                        <th>成績</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for(var i=0; i< schoolData['grade'].length ; i++) {%>
                                        <tr>
                                            <td> <%= i+1 %></td>
                                            <td> <%= schoolData['grade'][i].semester %></td>
                                            <td> <%= schoolData['grade'][i].className %></td>
                                            <td> <%= schoolData['grade'][i].grade %></td>
                                        </tr>
                                    <% } %>
                                </tbody>
                        </table>
                        <div class="">
                            <p class="media-body pb-3 mb-0 lh-125 border-bottom border-gray">
                                <strong class="d-block text-gray-dark text-muted">Rank:</strong>
                            </p>
                            <table class="table table-striped table-sm small">
                                <thead>
                                    <tr>
                                        <th>學年度學期</th>
                                        <th>排名/全校人數</th>
                                    </tr>
                                </thead>

                                <tbody>
                                    <% for(var i=0; i< schoolData['rank'].length ; i++) {%>
                                        <tr>
                                            <th> <%= schoolData['rank'][i].semester %></th>
                                            <th> <%= schoolData['rank'][i].rank %></th>
                                        </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div> 
                    </div>
                    <div class="d-flex align-items-center p-3 text-white-50 bg-primary rounded shadow-sm">
                        <div class="lh-100">
                            <h6 class="mb-0 text-white lh-100">E-portfolio</h6>
                            <small>from <%= schoolData['studentInfo'].highSchool %></small>
                        </div>
                    </div>
                    <div class="bg-white rounded shadow-sm">
                        <div class="media text-muted pt-3">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-file-earmark-pdf" viewBox="0 0 16 16">
                            <path d="M14 14V4.5L9.5 0H4a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2zM9.5 3A1.5 1.5 0 0 0 11 4.5h2V14a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h5.5v2z"></path>
                            <path d="M4.603 14.087a.81.81 0 0 1-.438-.42c-.195-.388-.13-.776.08-1.102.198-.307.526-.568.897-.787a7.68 7.68 0 0 1 1.482-.645 19.697 19.697 0 0 0 1.062-2.227 7.269 7.269 0 0 1-.43-1.295c-.086-.4-.119-.796-.046-1.136.075-.354.274-.672.65-.823.192-.077.4-.12.602-.077a.7.7 0 0 1 .477.365c.088.164.12.356.127.538.007.188-.012.396-.047.614-.084.51-.27 1.134-.52 1.794a10.954 10.954 0 0 0 .98 1.686 5.753 5.753 0 0 1 1.334.05c.364.066.734.195.96.465.12.144.193.32.2.518.007.192-.047.382-.138.563a1.04 1.04 0 0 1-.354.416.856.856 0 0 1-.51.138c-.331-.014-.654-.196-.933-.417a5.712 5.712 0 0 1-.911-.95 11.651 11.651 0 0 0-1.997.406 11.307 11.307 0 0 1-1.02 1.51c-.292.35-.609.656-.927.787a.793.793 0 0 1-.58.029zm1.379-1.901c-.166.076-.32.156-.459.238-.328.194-.541.383-.647.547-.094.145-.096.25-.04.361.01.022.02.036.026.044a.266.266 0 0 0 .035-.012c.137-.056.355-.235.635-.572a8.18 8.18 0 0 0 .45-.606zm1.64-1.33a12.71 12.71 0 0 1 1.01-.193 11.744 11.744 0 0 1-.51-.858 20.801 20.801 0 0 1-.5 1.05zm2.446.45c.15.163.296.3.435.41.24.19.407.253.498.256a.107.107 0 0 0 .07-.015.307.307 0 0 0 .094-.125.436.436 0 0 0 .059-.2.095.095 0 0 0-.026-.063c-.052-.062-.2-.152-.518-.209a3.876 3.876 0 0 0-.612-.053zM8.078 7.8a6.7 6.7 0 0 0 .2-.828c.031-.188.043-.343.038-.465a.613.613 0 0 0-.032-.198.517.517 0 0 0-.145.04c-.087.035-.158.106-.196.283-.04.192-.03.469.046.822.024.111.054.227.09.346z"></path>
                        </svg>
                        <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray" style="margin-left:10px">
                            <strong class="d-block text-gray-dark">@username</strong>
                            2021/9/25上傳 完成認證
                        </p>
                        </div>
                    </div>
                <% } %>
                <% if(typeof award != "undefined" && award){ %>
                    <div class="d-flex align-items-center p-3 text-white-50 bg-primary rounded shadow-sm">
                        <div class="lh-100">
                            <h6 class="mb-0 text-white lh-100">Award & Experience</h6>
                            <small>from certificate channel</small>
                        </div>
                    </div>
                    <% for(var i=0; i< award.length ; i++) {%>
                        <div class="bg-white rounded shadow-sm">
                            <div class="media text-muted pt-3">
                            <% if(award[i].data.type == "class"){ %> 
                                <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-book" viewBox="0 0 16 16">
                                    <path d="M1 2.828c.885-.37 2.154-.769 3.388-.893 1.33-.134 2.458.063 3.112.752v9.746c-.935-.53-2.12-.603-3.213-.493-1.18.12-2.37.461-3.287.811V2.828zm7.5-.141c.654-.689 1.782-.886 3.112-.752 1.234.124 2.503.523 3.388.893v9.923c-.918-.35-2.107-.692-3.287-.81-1.094-.111-2.278-.039-3.213.492V2.687zM8 1.783C7.015.936 5.587.81 4.287.94c-1.514.153-3.042.672-3.994 1.105A.5.5 0 0 0 0 2.5v11a.5.5 0 0 0 .707.455c.882-.4 2.303-.881 3.68-1.02 1.409-.142 2.59.087 3.223.877a.5.5 0 0 0 .78 0c.633-.79 1.814-1.019 3.222-.877 1.378.139 2.8.62 3.681 1.02A.5.5 0 0 0 16 13.5v-11a.5.5 0 0 0-.293-.455c-.952-.433-2.48-.952-3.994-1.105C10.413.809 8.985.936 8 1.783z"></path>
                                </svg>
                            <% }else if(award[i].data.type == "exam"){ %>
                                <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-card-heading" viewBox="0 0 16 16">
                                    <path d="M14.5 3a.5.5 0 0 1 .5.5v9a.5.5 0 0 1-.5.5h-13a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h13zm-13-1A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h13a1.5 1.5 0 0 0 1.5-1.5v-9A1.5 1.5 0 0 0 14.5 2h-13z"></path>
                                    <path d="M3 8.5a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 0 1h-9a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 0 1 0 1h-6a.5.5 0 0 1-.5-.5zm0-5a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5v-1z"></path>
                                </svg>
                            <% }else if(award[i].data.type == "contest"){ %>
                                <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-award" viewBox="0 0 16 16">
                                    <path d="M9.669.864 8 0 6.331.864l-1.858.282-.842 1.68-1.337 1.32L2.6 6l-.306 1.854 1.337 1.32.842 1.68 1.858.282L8 12l1.669-.864 1.858-.282.842-1.68 1.337-1.32L13.4 6l.306-1.854-1.337-1.32-.842-1.68L9.669.864zm1.196 1.193.684 1.365 1.086 1.072L12.387 6l.248 1.506-1.086 1.072-.684 1.365-1.51.229L8 10.874l-1.355-.702-1.51-.229-.684-1.365-1.086-1.072L3.614 6l-.25-1.506 1.087-1.072.684-1.365 1.51-.229L8 1.126l1.356.702 1.509.229z"></path>
                                    <path d="M4 11.794V16l4-1 4 1v-4.206l-2.018.306L8 13.126 6.018 12.1 4 11.794z"></path>
                                </svg>
                            <% } %>
                            <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray" style="margin-left:10px">
                                <strong class="d-block text-gray-dark"> <%= award[i].data.activity %></strong>
                                <%= award[i].data.data %><br>
                                <%= award[i].data.createdAt.substring(0,10) %>獲得
                            </p>
                            </div>
                        </div>
                    <% } %>
                <% } %>
                <div class="bg-white rounded shadow-sm">
                    <div class="media text-muted pt-3">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-award" viewBox="0 0 16 16">
                            <path d="M9.669.864 8 0 6.331.864l-1.858.282-.842 1.68-1.337 1.32L2.6 6l-.306 1.854 1.337 1.32.842 1.68 1.858.282L8 12l1.669-.864 1.858-.282.842-1.68 1.337-1.32L13.4 6l.306-1.854-1.337-1.32-.842-1.68L9.669.864zm1.196 1.193.684 1.365 1.086 1.072L12.387 6l.248 1.506-1.086 1.072-.684 1.365-1.51.229L8 10.874l-1.355-.702-1.51-.229-.684-1.365-1.086-1.072L3.614 6l-.25-1.506 1.087-1.072.684-1.365 1.51-.229L8 1.126l1.356.702 1.509.229z"></path>
                            <path d="M4 11.794V16l4-1 4 1v-4.206l-2.018.306L8 13.126 6.018 12.1 4 11.794z"></path>
                        </svg>
                      <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray" style="margin-left:10px">
                        <strong class="d-block text-gray-dark">全國自然類科學展覽</strong>
                        佳作 題目:幾何配數歸，碰出新滋味 <br>
                        2021/9/25獲得
                      </p>
                    </div>
                </div>
                <div class="bg-white rounded shadow-sm">
                    <div class="media text-muted pt-3">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-card-heading" viewBox="0 0 16 16">
                            <path d="M14.5 3a.5.5 0 0 1 .5.5v9a.5.5 0 0 1-.5.5h-13a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h13zm-13-1A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h13a1.5 1.5 0 0 0 1.5-1.5v-9A1.5 1.5 0 0 0 14.5 2h-13z"></path>
                            <path d="M3 8.5a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 0 1h-9a.5.5 0 0 1-.5-.5zm0 2a.5.5 0 0 1 .5-.5h6a.5.5 0 0 1 0 1h-6a.5.5 0 0 1-.5-.5zm0-5a.5.5 0 0 1 .5-.5h9a.5.5 0 0 1 .5.5v1a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5v-1z"></path>
                        </svg>
                      <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray" style="margin-left:10px">
                        <strong class="d-block text-gray-dark">TOEIC</strong>
                        725分 2020/8/17獲得
                      </p>
                    </div>
                </div>
                <div class="bg-white rounded shadow-sm">
                    <div class="media text-muted pt-3">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" fill="currentColor" class="bi bi-book" viewBox="0 0 16 16">
                            <path d="M1 2.828c.885-.37 2.154-.769 3.388-.893 1.33-.134 2.458.063 3.112.752v9.746c-.935-.53-2.12-.603-3.213-.493-1.18.12-2.37.461-3.287.811V2.828zm7.5-.141c.654-.689 1.782-.886 3.112-.752 1.234.124 2.503.523 3.388.893v9.923c-.918-.35-2.107-.692-3.287-.81-1.094-.111-2.278-.039-3.213.492V2.687zM8 1.783C7.015.936 5.587.81 4.287.94c-1.514.153-3.042.672-3.994 1.105A.5.5 0 0 0 0 2.5v11a.5.5 0 0 0 .707.455c.882-.4 2.303-.881 3.68-1.02 1.409-.142 2.59.087 3.223.877a.5.5 0 0 0 .78 0c.633-.79 1.814-1.019 3.222-.877 1.378.139 2.8.62 3.681 1.02A.5.5 0 0 0 16 13.5v-11a.5.5 0 0 0-.293-.455c-.952-.433-2.48-.952-3.994-1.105C10.413.809 8.985.936 8 1.783z"></path>
                        </svg>
                      <p class="media-body pb-3 mb-0 small lh-125 border-bottom border-gray" style="margin-left:10px">
                        <strong class="d-block text-gray-dark">Udemy(Python and Django Full Stack Web Developer Bootcamp)</strong>
                        累積課程時數35小時
                      </p>
                    </div>
                </div>
               
            </main>
        </div>
    </div>
<%- include('footer') -%>